apiVersion: v1
kind: Config
current-context: default
preferences: {}
clusters:
- name: kubernetes
  cluster:
    certificate-authority: {{ kubernetes_home }}/ssl/ca.pem
    server: {{ kube_apiserver_address }}

contexts:
- name: default
  context:
    cluster: kubernetes
    user: {{ item.context_user }}

users:
- name: kubelet-bootstrap
  user:
    token: {{ kubelet_bootstrap_token }} 

