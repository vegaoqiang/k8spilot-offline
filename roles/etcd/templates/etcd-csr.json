{
  "CN": "etcd",
  "hosts": [
    {% for host in groups['etcd'] %}
    "{{ hostvars[host].ansible_facts.default_ipv4.address }}",
    {% endfor %}
    "127.0.0.1"
  ],
  "key": {
    "algo": "rsa",
    "size": 2048
  },
  "names": [
    {
      "C": "CN",
      "L": "K8SPILOT",
      "O": "etcd"
    }
  ]
}
